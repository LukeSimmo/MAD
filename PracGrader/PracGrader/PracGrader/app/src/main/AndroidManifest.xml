<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="curtin.edu.pracgrader">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PracGrader">
        <activity android:name=".OptionsActivity"></activity>
        <activity android:name=".optionsDisplayActivity"></activity>
        <activity android:name=".EditEntryActivity"></activity>
        <activity android:name=".EditPracticalActivity"></activity>
        <activity android:name=".MarkStudentsActivity"></activity>
        <activity android:name=".viewInstructorDetailsActivity"></activity>
        <activity android:name=".viewPracticalDetailsActivity"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>