<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.assignment2">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Assignment2">
        <activity android:name="curtin.edu.assignment2.UserActivity"></activity>
        <activity android:name="curtin.edu.assignment2.RegisterStudentActivity"></activity>
        <activity android:name="curtin.edu.assignment2.EditStudentActivity"></activity>
        <activity android:name="curtin.edu.assignment2.ViewStudentActivity"></activity>
        <activity android:name="curtin.edu.assignment2.TestActivity"></activity>
        <activity android:name="curtin.edu.assignment2.SelectStudentActivity"></activity>
        <activity android:name="curtin.edu.assignment2.GridPhotoActivity"></activity>
        <activity android:name="curtin.edu.assignment2.ViewResults"></activity>
        <activity android:name="curtin.edu.assignment2.SendEmailActivity"></activity>
        <activity
            android:name="curtin.edu.assignment2.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>